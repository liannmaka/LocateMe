{
  "name": "locateme",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy-dev": "s3-deploy './dist/{js/*,css/*,img/*,index.html}' --cwd './dist/' --region eu-north-1 --bucket locate-me --profile {x}",
    "release-dev": "npm run build && npm run deploy-dev"
  },
  "dependencies": {
    "maplibre-gl": "^4.5.0",
    "pinia": "^2.1.7",
    "s3-bucket": "^1.0.3",
    "s3-deploy": "^1.4.0",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "vite": "^5.3.1"
  }
}
